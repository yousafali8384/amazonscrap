      function add() {
        seconds++;
        if (seconds >= 60) {
          seconds = 0;
          minutes++;
          if (minutes >= 60) {
            minutes = 0;
            hours++;
          }
        }

        h1.textContent =
          (hours ? (hours > 9 ? hours : "0" + hours) : "00") +
          ":" +
